            android:text="班        级:"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/stuupdateTV7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/stuupdateET7"
            android:layout_alignBottom="@+id/stuupdateET7"
            android:layout_alignParentLeft="true"
            android:text="电子邮箱:"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/stuupdateTV6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/stuupdateET6"
            android:layout_alignBottom="@+id/stuupdateET6"
            android:layout_alignParentLeft="true"
            android:text="电        话:"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/stuupdateTV5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/stuupdateET5"
            android:layout_alignBottom="@+id/stuupdateET5"
            android:layout_alignParentLeft="true"
            android:text="性        别:"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/stuupdateTV4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="账号类型:"
            android:textSize="20dp"
            android:layout_alignBaseline="@+id/stuupdateET4"
            android:layout_alignBottom="@+id/stuupdateET4"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/stuupdateTV3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="密        码:"
            android:textSize="20dp"
            android:layout_alignBaseline="@+id/stuupdateET3"
            android:layout_alignBottom="@+id/stuupdateET3"
            android:layout_alignParentStart="true" />
        <Button
            android:id="@+id/stuupdatebtn"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/stuupdateET11"
            android:layout_toRightOf="@+id/stuupdateTV11"
            android:background="@drawable/btn"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:onClick="stuInsertLoad"
            android:text="更新"
            android:textSize="25dp"
            android:padding="1dp"/>


    </RelativeLayout>

</ScrollView>
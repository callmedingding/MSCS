<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:scrollbars="none" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp" >

        <TextView
            android:id="@+id/stuupdateTV0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:paddingTop="10dp"
            android:layout_toRightOf="@+id/stuupdateTV11"
            android:text="修改用户信息"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/stuupdateTV1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/stuupdateET1"
            android:layout_alignBottom="@+id/stuupdateET1"
            android:text="账        号:"
            android:textSize="20dp" />



        <TextView
            android:id="@+id/stuupdateTV2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/stuupdateET1"
            android:text="姓        名:"
            android:textSize="20dp" />
        <EditText
            android:id="@+id/stuupdateET11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/stuupdateET10"
            android:layout_toRightOf="@+id/stuupdateTV11"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true" />
        <EditText
            android:id="@+id/stuupdateET10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/stuupdateET9"
            android:layout_toRightOf="@+id/stuupdateTV10"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET11" />
        <EditText
            android:id="@+id/stuupdateET9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/stuupdateET8"
            android:layout_toRightOf="@+id/stuupdateTV10"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET10" />

        <EditText
            android:id="@+id/stuupdateET8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/stuupdateET7"
            android:layout_toRightOf="@+id/stuupdateTV4"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET9" />

        <EditText
            android:id="@+id/stuupdateET7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/stuupdateET6"
            android:layout_below="@+id/stuupdateET6"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET8"
            android:layout_toRightOf="@+id/stuupdateTV4"/>

        <EditText
            android:id="@+id/stuupdateET6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/stuupdateET5"
            android:layout_below="@+id/stuupdateET5"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET8"
            android:layout_toRightOf="@+id/stuupdateTV4"/>

        <EditText
            android:id="@+id/stuupdateET5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/stuupdateET4"
            android:layout_toRightOf="@+id/stuupdateTV4"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET8" />

        <EditText
            android:id="@+id/stuupdateET4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/stuupdateET3"
            android:layout_toRightOf="@+id/stuupdateTV4"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET5" />

        <EditText
            android:id="@+id/stuupdateET3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/stuupdateET2"
            android:layout_below="@+id/stuupdateET2"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET4"
            android:layout_toRightOf="@+id/stuupdateTV4"/>

        <EditText
            android:id="@+id/stuupdateET2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/stuupdateTV2"
            android:layout_toRightOf="@+id/stuupdateTV4"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET4" />

        <EditText
            android:id="@+id/stuupdateET1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/stuupdateTV0"
            android:layout_below="@+id/stuupdateTV0"
            android:layout_marginTop="17dp"
            android:ems="10"
            android:gravity="top"
            android:inputType="textMultiLine|none"
            android:singleLine="true"
            android:layout_alignEnd="@+id/stuupdateET2"
            android:layout_toRightOf="@+id/stuupdateTV4"/>
        <TextView
            android:id="@+id/stuupdateTV11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/stuupdateET11"
            android:layout_alignBottom="@+id/stuupdateET11"
            android:layout_alignParentLeft="true"
            android:text="可选课程类型:"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/stuupdateTV10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/stuupdateET10"
            android:layout_alignBottom="@+id/stuupdateET10"
            android:layout_alignParentLeft="true"
            android:text="账号状态:"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/stuupdateTV9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/stuupdateET9"
